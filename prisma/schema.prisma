generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model Inventory {
  id          Int     @default(autoincrement()) @id
  name        String
  count       Int
}

model ArchivedInventory {
  id                Int @default(autoincrement()) @id
  deletionComment   String
  name              String
  count             Int
}
